{
  "metadata": [
    {
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "src": [
        {
          "src": "./",
          "files": [
            "**/*.csproj"
          ]
        }
      ]
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "default(zh-cn)",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "",
      "_appTitle": "",
      "_enableSearch": true,
      "pdf": true
    }
  }
}